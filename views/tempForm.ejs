                                    <input type="radio" id="<%= field.id %>-<%= j %>_<%=formType%>" name="<%= field.id %>" value="<%= field.options[j] %>">
                                    <label class="ms-3" for="<%= field.id %>-<%= j %>_<%=formType%>"><%= field.options[j] %></label><br>

                      <%}%>
                      <% if (createdFieldsFlag == "true") { %>
                        <div class="created-fields-container_<%=formType%>">
                          <% for (let i = 0; i < createdFields.length; i++) { %>
                            <% let field = createdFields[i]; %>
                            <div is-req="<%=field.required%>" id="<%=field.id%>_<%=formType%>" type="<%=field.type%>" question="<%=field.label%>">

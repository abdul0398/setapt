<div class="agentEditPopup close_popup">
  <span onclick="closeAllPopups()" id="close_popup_btn"
    ><i class="fa-solid fa-x"></i
  ></span>
  <h5 class="text-center">Edit Client</h5>
  <div class="form-group">
    <input
      type="text"
      class="form-control border p-1 mt-1 border-1"
      id="agent-name-edit"
      placeholder="Enter Name"
    />
  </div>
  <div class="form-group">
    <input
      type="email"
      class="form-control border p-1 mt-1 border-1 bg-light"
      id="agent-email-edit"
      placeholder="Enter Email"
      disabled
    />
  </div>
  <div class="form-group">
    <input
      type="password"
      class="form-control border p-1 mt-1 border-1"
      id="agent-password-edit"
      placeholder="Enter Password"
    />
  </div>
  <div class="form-group">
    <input
      type="password"
      class="form-control border p-1 mt-1 border-1"
      id="agent-confirm-password-edit"
      placeholder="Confirm Password"
    />
  </div>
  <div class="d-flex mt-3">
    <button
      id="agent-edit-post-btn"
      class="btn mx-auto"
      onclick="editClientPost()"
    >
      Save
    </button>
    <a
      id="agent-delete-btn"
      onclick="deleteAgent(this)"
      class="btn btn-danger me-auto"
    >
      Delete
    </a>
  </div>
</div>
